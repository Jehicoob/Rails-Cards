.view-game-modal
  %h3 Quiz Complete!
  %p= @deck.name 
  %p.deck-score
  .graph-content
    .percent-content
      %p.percent
      %p Accuracy
    = cardscore_by_status 
  %p.succesfull_cards
  .modal-qualification
    = render 'qualification'
  .modal-buttons
    = link_to 'Try Again', game_path(deck_code: @deck.code)
    = link_to 'Done', deck_path(@deck)